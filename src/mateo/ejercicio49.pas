program ejercicio49;
uses crt;
var a,b,d,c,e:integer;
begin
        clrscr;
        writeln('ingrese 5 numeros, luego de cada uno presione enter: ');
        readln(a,b,c,d,e);

        writeln(a,' * ', 1, ' = ',a*1);
        writeln(b,' * ', 2, ' = ',b*2);
        writeln(c,' * ', 3, ' = ',c*3);
        writeln(d,' * ', 4, ' = ',d*4);
        writeln(e,' * ', 5, ' = ',e*5);
        readln;
end.